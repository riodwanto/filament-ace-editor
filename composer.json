{
    "name": "riodwanto/filament-ace-editor",
    "description": "An ACE editor field for Filament forms with syntax highlighting, themes, and autocompletion.",
    "version": "4.0.1",
    "type": "filament-plugin",
    "keywords": [
        "filament",
        "filament-forms",
        "filament-field",
        "code-editor",
        "syntax-highlighting",
        "ace-editor",
        "laravel",
        "php",
        "admin-panel",
        "form-field",
        "code-editing",
        "autocomplete",
        "dark-mode",
        "themes",
        "livewire",
        "ace-editor"
    ],
    "homepage": "https://github.com/riodwanto/filament-ace-editor",
    "support": {
        "issues": "https://github.com/riodwanto/filament-ace-editor/issues",
        "source": "https://github.com/riodwanto/filament-ace-editor"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rio Dewanto P",
            "email": "rdp.tech@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^11.28"
    },
    "require-dev": {
        "filament/filament": "^4.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Riodwanto\\FilamentAceEditor\\": "src/",
            "Riodwanto\\FilamentAceEditor\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Riodwanto\\FilamentAceEditor\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Riodwanto\\FilamentAceEditor\\FilamentAceEditorServiceProvider"
            ],
            "aliases": {
                "FilamentAceEditor": "Riodwanto\\FilamentAceEditor\\Facades\\FilamentAceEditor"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
